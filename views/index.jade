extends layout

block content
    .container
        h1(class="header")= title
        form(action="album", method="post", enctype="multipart/form-data")
            .form-group
                label(for="album_csv") upload .csv file with album information
                input(type="file", name="album_csv")
            .form-group
                input(type="submit" value="Submit" class="btn btn-primary")